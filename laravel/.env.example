APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://127.0.0.1:8080/_app

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3307
DB_DATABASE=your_db_name
DB_USERNAME=your_db_user
DB_PASSWORD=your_db_password

LEGACY_DB_HOST=127.0.0.1
LEGACY_DB_PORT=3307
LEGACY_DB_DATABASE=your_legacy_db_name
LEGACY_DB_USERNAME=your_legacy_readonly_user
LEGACY_DB_PASSWORD=your_legacy_readonly_password

# Optional: set only when enabling migrated write endpoints
LEGACY_WRITE_DB_URL=
LEGACY_WRITE_DB_HOST=127.0.0.1
LEGACY_WRITE_DB_PORT=3307
LEGACY_WRITE_DB_DATABASE=your_legacy_db_name
LEGACY_WRITE_DB_USERNAME=your_legacy_write_user
LEGACY_WRITE_DB_PASSWORD=your_legacy_write_password

SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync

CACHE_STORE=file
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

MIGRATE_REQUESTS_MODULE=false
MIGRATE_REQUESTS_FETCH_SUBCATEGORY=

MIGRATE_PROPOSAL_PRICING_CHECK=false
MIGRATE_APIS_INDEX=false
MIGRATE_REQUESTS_PAUSE_REQUEST=
MIGRATE_REQUESTS_ACTIVE_REQUEST=
MIGRATE_REQUESTS_MANAGE_REQUESTS=
MIGRATE_REQUESTS_RESUME_REQUEST=
MIGRATE_REQUESTS_CREATE_REQUEST=
MIGRATE_REQUESTS_UPDATE_REQUEST=
