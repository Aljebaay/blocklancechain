{
    "name": "gig-zone/app",
    "description": "gig-zone marketplace application",
    "license": "proprietary",
    "require": {
        "stripe/stripe-php": "^7.45",
        "opentok/opentok": "^4.21",
        "guzzlehttp/guzzle": "^7.4.5",
        "yabacon/paystack-php": "^2.2",
        "phpmailer/phpmailer": "^6.10",
        "aws/aws-sdk-php": "^3.369",
        "mercadopago/dx-php": "^2.0",
        "twilio/sdk": "^6.10",
        "cboden/ratchet": "^0.4.4",
        "symfony/http-foundation": "^5.4.50"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "audit:endpoints": "php scripts/audit_endpoints.php",
        "generate:public-stubs": "php scripts/generate_compat_stubs.php",
        "smoke:http": "php scripts/smoke_http.php",
        "test:security-regression": "php tests/regression/security_checks.php",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "php -S 127.0.0.1:8080 -t public public/router.php"
        ]
    }
}
